package com.cor.graphx.event

import com.cor.graphx.util.VertexProperty

/**
 * Class for event Crash
 */

case class CrashEvent(timestamp: Long, motorbikeId: Integer, location: String, speed_a1: Double, speed_a2: Double, currentTimestamp1: Long, currentTimestamp2: Long) extends VertexProperty(timestamp) {
  /**
   * toString Method
   */
  override def toString(): String = {
    return "CrashEvent [" + timestamp + "," + motorbikeId + "," + location + "," + speed_a1 + "," + speed_a2 + "]";
  }

}